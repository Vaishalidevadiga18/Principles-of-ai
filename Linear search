def linear_Search(arr, n, key):  
  
    
    for i in range(0, n):  
        if (arr[i] == key):  
            return i  
    return -1  
  
arr = [1 ,32, 5, 41, 7, 9]  
key = 41  
  
n = len(arr)  
res = linear_Search(arr, n, key)  
if(res == -1):  
    print("Element not found")  
else:  
    print("Element found at index: ", res)  
